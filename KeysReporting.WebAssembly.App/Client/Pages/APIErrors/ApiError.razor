@page "/API/Error"
<PageTitle>Keys360 Reporting - API Error</PageTitle>
<PageDisplayTitle Title="API Errors" Sub="Errors from API." />

<section class="section">
    <div class="columns is-centered">
        <div class="column is-10">
            <div class="tile is-ancestor">
                <div class="tile is-parent">
                    <div class="tile is-child box">
                        <p class="title">API Errors</p>
                        <ApiErrorTable ApiErrors="_errorList" TotalSize="TotalSize" OnScroll="LoadApiError" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>